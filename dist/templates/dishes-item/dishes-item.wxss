
/* 菜品 */
.dishe-img {
  width: 100%;
  height: 200rpx; 
}
.dishe-panel {
  width: 43%;
  background-color: white;
  padding: 20rpx;
  display: inline-block;
  border: 1px #E5E5E5 solid;
  margin-bottom: 20rpx;
  font-size: 30rpx;
}
.dishe-panel .price {
  display: inline-block;
  color: #FF0000;
  float: right;
  font-size: 26rpx;
  text-align: right;
  margin-top: 8rpx;
}
.next-icon {
  font-size: 26rpx;
  color: gray;
}
.right-icon {
  width: 25rpx;
  height: 25rpx;
  position: absolute;
  margin-top: 9rpx;
}