/* pages/talentDetails/talentMoreVideo.wxss */
@import '../../utils/weSwiper/weSwiper.wxss';

page {
  height: 100%;
  width: 100%;
}
.body {
    width: 100%;
    height: 100%;
    /* background-color: #000000; */
    font-size: 30rpx;
}

.videoList-view { 
    background-color: white;
    padding: 20rpx;
    height: 100%;
}
.title {
    margin-bottom: 20rpx;
}
.video-view image{ 
    width: 48.5%;
    height: 200rpx;
    margin-bottom: 20rpx;
}
.videoListScrollView {
    background-color: pink;
}

/* 浏览视频 */
.showVideo {
    background-color: #000000; 
    height: 100%; 
    width: 100%;
    color: white;
}
.we-wrapper {
    margin: auto;
}
.we-container {
    height: 85%;
}
.numberText {
    font-size: 13px;
    padding: 2.5%;
    float: left;
    width: 95%;
    height: 5%;
}
.numberText text {
    font-size: 15px;
}
.hiddenVideo {
    float: right;
    color: white;
}

.foot-view {
    text-align: left;
    padding-left: 20rpx;
    color: white;
    height: 5%;
}

.swiper {
    width: 100%;
    height: 100%;
    background-color: black;
}
.video {
    width: 100%;
    height: 50%; 
}



